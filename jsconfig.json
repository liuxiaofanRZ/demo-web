{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "types": ["vite/client","ant-design-vue/typings/global"],
    "paths": { "@/*":["./src/*"] }
  },
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.vue"]
}